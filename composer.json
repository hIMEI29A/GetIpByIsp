{
    "name": "himei/get-ip-by-isp",
    "description": "Simple cURL based console application for getting IP ranges from suip.biz web-services by city, country or ISP",
    "keywords": [
        "console",
        "cli",
        "curl",
        "ip ranges"
    ],
    "homepage": "https://github.com/hlmel/getIpbyIsp",
    "license": "MIT",
    "authors": [
        {
            "name": "hIMEI",
            "email": "himei@tuta.io"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "pear/console_commandline": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "GetIpByIsp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GetIpByIsp\\": "tests/"
        }
    },
    "scripts": {
        "test": ["phpunit"]
    }
}
